{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "regions": ["lhr1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Platform",
          "value": "WebWaka"
        },
        {
          "key": "X-Governance-Version",
          "value": "2.0"
        }
      ]
    }
  ],
  "rewrites": [],
  "redirects": []
}
